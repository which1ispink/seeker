{
    "name": "seeker/seeker",
    "description": "Seeker is a minimal PHP framework",
    "type": "library",
    "keywords": [
        "php",
        "framework"
    ],
    "authors": [
        {
            "name": "Ahmed Hassan",
            "email": "a.hassan.dev@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "zendframework/zend-http": "^2.5",
        "zendframework/zend-db": "^2.8",
        "zendframework/zend-debug": "^2.5",
        "zendframework/zend-coding-standard": "^1.0",
        "zendframework/zend-dom": "^2.6",
        "zendframework/zend-mail": "^2.7",
        "zendframework/zend-paginator": "^2.7",
        "zendframework/zend-session": "^2.7",
        "zendframework/zend-uri": "^2.5",
        "zendframework/zend-validator": "^2.8",
        "zendframework/zend-di": "^2.6",
        "philo/laravel-blade": "^3.1",
        "beberlei/assert": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "application/",
            "Seeker\\": "core/"
        }
    },
    "scripts": {
        "serve": "php -S localhost:8080 -t public/ public/index.php",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
