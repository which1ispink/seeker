{
    "name": "seeker/seeker",
    "description": "Seeker is a minimal PHP framework",
    "type": "library",
    "keywords": [
        "php",
        "framework"
    ],
    "authors": [
        {
            "name": "Ahmed Hassan",
            "email": "a.hassan.dev@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "zendframework/zend-coding-standard": "^1.0",
        "zendframework/zend-db": "^2.8",
        "philo/laravel-blade": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "application/",
            "Seeker\\": "core/"
        }
    },
    "scripts": {
        "serve": "php -S localhost:8080 -t public/ public/index.php",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
